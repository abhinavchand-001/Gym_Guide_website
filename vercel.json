{
  "version": 2,
  "builds": [
    {
      "src": "**/*.{html,css,js,png,jpg,jpeg,svg,ico}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/text.html"
    },
    {
      "src": "/(.*\\.(?:css|js|png|jpg|jpeg|svg|ico))",
      "dest": "/$1"
    }
  ]
}
